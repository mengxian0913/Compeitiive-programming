#pragma GCC optimize("O3")
#include <bits/stdc++.h>
using namespace std;
#define int long long
#define ff first
#define ss second
#define fastIO cin.tie(nullptr)->sync_with_stdio(false);
#define INF 0x7FFFFFFF
#define pb push_back
#define all(aa) aa.begin(),aa.end()
#define MOD (int)(1e9+7)

#define MAXN 1005

int mv_x[] = {0, 0, 1, -1};
int mv_y[] = {1, -1, 0, 0};
string mp[MAXN];
bool vis[MAXN][MAXN];
int n, m;


bool check(int y, int x) {
  if(x < 0 || x >= m) {
    return 0;
  }

  if(y < 0 || y >= n) {
    return 0;
  }

  return (vis[y][x]);
}


void dfs(int y, int x) {
  for(int i = 0; i < 4; i++) {
    int now_y = y + mv_y[i];
    int now_x = x + mv_x[i];
    if(check(now_x, now_y)) {
      vis[now_x][now_y] = true;
      dfs(now_x, now_y);
    }
   }
  return;
}


void solve(){
  memset(vis, 0, sizeof(vis)); 
  cin >> n >> m;
  for(int i = 0; i < n; i++) {
    cin >> mp[i];
  }

  int ans = 0;

  for(int i = 0; i < n; i++) {
    for(int j = 0; j < m; j++) {

      if(i == 1 && j == 4) {
        cout << vis[i][j] << "\n" << mp[i][j] << "\n";
      }

      if(mp[i][j] == '#'){
          vis[i][j] = 1;
      }

      else if(!vis[i][j]){
        cout << i << " " << j << "\n";
        ans++;
        vis[i][j] = true;
        dfs(i, j);
      }
    }
  }

  cout << ans << "\n";
  return;
}

signed main(){
  fastIO
  solve();
  return 0;
}
